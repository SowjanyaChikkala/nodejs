const fs=require('fs')
fs.stat("./model.js",(err,stats)=>{
  if(err) throw err;
  //console.log(stats)
  console.log(stats.isFile())
  console.log(stats.isDirectory())
  console.log(stats.isBlockDevice())
  console.log(stats.isCharacterDevice())  
  console.log(stats.isSocket())
  console.log(stats.isFile())

})
